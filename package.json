{
  "name": "redcloud",
  "formattedName": "RedCloud",
  "version": "0.2.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "pkg": "pkg",
    "nexe": "nexe",
    "asar": "asar",
    "electron": "electron",
    "electron-packager": "electron-packager",
    "x": "node --unhandled-rejections=strict",
    "postinstall": "yarn x ./bin/postinstall",
    "db:compact": "node ./bin/db/compact",
    "dev:client": "concurrently --kill-others -s -c cyan,yellow -n \" COMPILE  , ELECTRON \" \"yarn x ./client/compile.js --watch true\" \"yarn electron ./client\"",
    "dev:server": "supervisor -q -w ./src ./src/app.js",
    "dev": "concurrently --kill-others -s -c blue,green -n \" SERVER , CLIENT \" \"yarn dev:server\" \"yarn dev:client\"",
    "production": "yarn x ./bin/run",
    "server": "yarn production --electron false",
    "compile": "yarn x ./client/compile.js",
    "build": "yarn x ./bin/build",
    "increment": "npm test && yarn x -e \"require('fs').writeFileSync('package.json', JSON.stringify({...(require('./package.json')), version: require('./package.json').version.split('.').map((v, i) => {if(i==2) return Number(v) + 1; return v}).join('.')}, null, 2))\""
  },
  "bin": "app.js",
  "author": "",
  "license": "ISC",
