{
  "name": "redcloud",
  "formattedName": "RedCloud",
  "version": "0.0.213",
  "description": "",
  "main": "app.js",
  "scripts": {
    "asdfasdf": "",
    "test": "echo Error: no test specified && exit 0",
    "dev": "node run.js",
    "api-hotreload-start": "npm run forever-restart && chokidar \"app.js\" \"src/**/*.js\" -c \"npm run release && npm run forever-restart\"",
    "pack": "webpack",
    "release": "npm test && node -e \"require('fs').writeFileSync('package.json', JSON.stringify({...(require('./package.json')), version: require('./package.json').version.split('.').map((v, i) => {if(i==2) return Number(v) + 1; return v}).join('.')}, null, 2))\"",
    "forever-start": "forever start --uid phub -a app.js --disable-cron",
    "forever-restart": "forever restart phub || npm run forever-start",
    "forever-stop": "forever stop phub",
    "start": "concurrently -c green,yellow,cyan,blue -n FRVR,PHUB,PACK,ELCT \"npm run api-hotreload-start\" \"tail -f ./../.forever/phub.log\" \"node webpack.js\" \"npm run electron\"",
    "oddit": "npm shrinkwrap && npm audit && del npm-shrinkwrap.json",
    "electron": "node_modules\\electron\\dist\\electron.exe ./src/components/electron.js",
    "production": "node_modules\\electron\\dist\\electron.exe app.js"
  },
