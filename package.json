    "forever": "^1.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.1.0",
    "fuse.js": "^6.2.0",
    "glob": "^7.1.6",
    "html-entities": "^1.2.1",
